<% content_for :body_class, "class=influencers-background" %>
<div class="influencers">
	<div class="container">
		<div class="grid">
			<div class="title">
				<h2><%= @category.name %> Influencers</h2>
			</div>
			<div class="list" id="category">
				<%= render partial: "influencers/profile", collection: @influencers %>
				<div class="clearfix"></div>
			</div>
		</div>
	</div> 
</div>